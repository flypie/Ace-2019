                             B      130 ; 04000h +-------+----------------------------------------------+
                             B      131 ;		|SYSVARS| DICT {12} DATA STACK ->			<- RET STACK | 1K RAM
                             B      132 ;		+=======+==============================================+
                             B      133 ;		|														|
